package F01;

public class MinTraad extends Thread {

	public MinTraad(String navn) {
		super(navn);
	}
	@Override 
	public void run() {
		while(true) {
			System.out.println(getName());
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}
