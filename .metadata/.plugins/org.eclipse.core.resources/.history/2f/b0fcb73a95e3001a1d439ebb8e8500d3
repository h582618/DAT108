import javax.swing.*;

public class Oppgave1 {

	public static void main(String[] args) throws InterruptedException {
	
	
		String s = "Start streng1";
		helloWorld helloWorld1 = new helloWorld();
		PrintThread thread2 = new PrintThread ();
	
		while(true) {
			
		
			
		helloWorld1.start();
		Thread.sleep(300);
		thread2.start();
		if( helloWorld1.getInput() != "Kake") {
			thread2.setStreng(helloWorld1.getInput());
		}
		System.out.println(helloWorld1.getInput());
		
		
		
		helloWorld1.stopp();
		thread2.stopp();

		}
		
		
	}
	
}
	

